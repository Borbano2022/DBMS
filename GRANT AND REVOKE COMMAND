CREATE USER 'Jesyl'
IDENTIFIED BY 'Alpha@localhost';

GRANT CREATE, ALTER, DROP
ON <Industrial_Technology_Prelim_output_2.*
TO 'Jesyl'

GRANT SELECT, INSERT, UPDATE, DELETE
ON Industrial_Technology_Prelim_output_2.*
TO 'Jesyl'

REVOKE SELECT, INSERT, UPDATE, DELETE, CREATE, ALTER, DROP
ON Industrial_Technology_Prelim_outpur_2.*
FROM 'Jesyl'
